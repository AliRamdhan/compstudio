{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "downlevelIteration": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [{
      "name": "next"
    }],
    "paths": {
      "@/laduny/*": ["./*"],
      "@/laduny-service/*": ["./laduny-service/*"],
      "@/laduny-track/*": ["./laduny-track/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "actions/auth.jsx", "reducers/message.jsx", "reducers/index.jsx", "reducers/auth.jsx"],
  "exclude": ["node_modules"]
}